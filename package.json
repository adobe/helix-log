{
  "name": "@adobe/helix-log",
  "version": "4.4.2",
  "description": "Logging for Helix",
  "main": "src/index.js",
  "scripts": {
    "test": "nyc --reporter=text --reporter=lcov --check-coverage --branches 1 --statements 29 --lines 32 mocha",
    "test-ci": "circleci tests glob 'test/*.test.js' | circleci tests split --split-by=timings | xargs nyc --reporter=text --reporter=lcov mocha -R mocha-junit-reporter && codecov",
    "lint": "./node_modules/.bin/eslint .",
    "docs": "npx jsdoc2md -t jsdoc2md/README.hbs src/*.js > README.md; echo",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/helix-log"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/adobe/helix-log/issues"
  },
  "homepage": "https://github.com/adobe/helix-log#readme",
  "dependencies": {
    "big.js": "^5.2.2",
    "colorette": "^1.1.0",
    "ferrum": "^1.4.1",
    "phin": "^3.4.0",
    "polka": "^0.5.2",
    "triple-beam": "^1.3.0",
    "uuid": "^3.3.3",
    "winston-transport": "^4.3.0"
  },
  "devDependencies": {
    "@adobe/eslint-config-helix": "1.1.0",
    "@semantic-release/changelog": "3.0.6",
    "@semantic-release/git": "8.0.0",
    "@semantic-release/github": "6.0.1",
    "ajv": "6.11.0",
    "bunyan": "1.8.12",
    "winston": "3.2.1",
    "codecov": "3.6.1",
    "eslint": "6.8.0",
    "eslint-plugin-header": "3.0.0",
    "eslint-plugin-import": "2.20.0",
    "jsdoc-to-markdown": "5.0.3",
    "junit-report-builder": "1.3.3",
    "lodash": "4.17.15",
    "mocha": "7.0.0",
    "mocha-junit-reporter": "1.23.3",
    "mocha-parallel-tests": "2.2.2",
    "nyc": "15.0.0",
    "semantic-release": "16.0.2"
  },
  "renovate": {
    "extends": [
      "config:base"
    ],
    "schedule": "at 07:00 pm on Monday",
    "timezone": "Europe/Zurich",
    "packageRules": [
      {
        "packagePatterns": [
          ".*"
        ],
        "groupName": "any"
      }
    ]
  }
}
